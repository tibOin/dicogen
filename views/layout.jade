doctype html
html(lang="fr")
  head
    meta(charset="UTF-8")
    
    title ESI_Share &copy;
    
    // Mobile Specific Meta
    meta(name="viewport" content="width=device-width, initial-scale=1")
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    
    // Bootstrap
    link(href="/assets/css/bootstrap.min.css" rel="stylesheet")
    
    // icon fonts font Awesome
    link(href="/assets/css/font-awesome.min.css" rel="stylesheet")
    
    // Custom Styles
    link(href="/assets/css/style.css" rel="stylesheet")
    
    //Color Style
    link(rel="stylesheet" type="text/css" id="color" href="/assets/css/colors/default.css")
    
    <!--[if lt IE 9]><script src="assets/js/html5shiv.js"></script><![endif]-->
    
    script(type="text/javascript", src="/assets/js/ua-parser.js")
    
  body
  
    // Preloader
    div(id="preloader")
      div(id="loader")
        div(class="dot")
        div(class="dot")
        div(class="dot")
        div(class="dot")
        div(class="dot")
        div(class="dot")
        div(class="dot")
        div(class="dot")
        div(class="lading")
      // /#preloader
    // Preloader End

    script(type="text/javascript").
      var parser = new UAParser();
      parser.setUA(navigator.userAgent);
      
      var result = parser.getResult();
      
      if(result.browser.name === 'Firefox') {
        alert('Cette version ne fonctionne malheureusement pas encore avec Firefox.\nChrome est bien mieux indiqué.\nSi vous ne l\'avez pas : https://www.google.fr/chrome/browser/desktop/');
      }
      else if (result.browser.name != 'Chrome') {
        alert('Vous utilisez ' + result.browser.name + '. Certaines fonctionnalités ne seront pas disponibles.\nChrome est bien mieux indiqué.\nSi vous ne l\'avez pas : https://www.google.fr/chrome/browser/desktop/');
      }
      
    // Main Menu
    div(id="main-menu" class="navbar navbar-default navbar-fixed-top" role="navigation")
    
      div(class="navbar-header")
        // responsive navigation
        button(type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse")
          span(class="sr-only") Toggle navigation
          i(class="fa fa-bars")
          
        nav(class="collapse navbar-collapse")
          // Main navigation
          ul(id="headernavigation" class="nav navbar-nav")
